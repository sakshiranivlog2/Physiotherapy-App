<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:cardCornerRadius="25dp"
    app:cardElevation="3dp"
    android:layout_marginTop="7dp"
    app:cardUseCompatPadding="true"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:id="@+id/linear_layout14"
        android:orientation="vertical"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:id="@+id/ll14"
            android:orientation="horizontal"
            android:layout_height="wrap_content"
            tools:ignore="UseCompoundDrawables">


            <TextView
                android:id="@+id/version14"
                android:layout_width="268dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="8dp"
                android:padding="16dp"
                android:text="Today's Income"
                android:textAlignment="viewStart"
                android:textColor="#0B0A0B"
                android:textSize="18sp"
                android:textStyle="bold" />


            <ImageView
                android:id="@+id/description14"
                android:layout_width="40dp"
                android:layout_height="63dp"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/ic_arrow_drop" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/expandable_layout14"
            android:layout_width="match_parent"
            android:layout_height="417dp"
            android:background="#E6E2E6">


            <LinearLayout
                android:id="@+id/f9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/f8"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="22dp"
                    android:layout_marginTop="10dp"
                    android:text="ITEM NAME"
                    android:textColor="@android:color/holo_red_dark"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="92dp"
                    android:layout_marginTop="10dp"
                    android:text="ITEM PRICE"
                    android:textColor="@android:color/holo_red_dark"
                    android:textSize="18sp"
                    android:textStyle="bold" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/f10"
                android:layout_width="match_parent"
                android:layout_height="58dp"
                android:layout_below="@+id/f9"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/f19"
                    android:layout_width="218dp"
                    android:layout_height="71dp"
                    android:layout_below="@+id/f9"
                    android:orientation="vertical">


                    <TextView
                        android:layout_width="107dp"
                        android:layout_height="32dp"
                        android:layout_marginLeft="23dp"
                        android:gravity="start"
                        android:text="MODALITIES"
                        android:textSize="12sp"
                        android:textStyle="bold" />


                    <TextView
                        android:layout_width="107dp"
                        android:layout_height="29dp"
                        android:layout_marginLeft="23dp"
                        android:background="@android:color/white"
                        android:gravity="center"
                        android:text="No Record Found"
                        android:textSize="12sp" />

                </LinearLayout>

                <EditText
                    android:layout_width="137dp"
                    android:textAlignment="center"
                    android:layout_height="match_parent"
                    android:textSize="12sp"
                    android:textStyle="bold" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/f11"
                android:layout_width="match_parent"
                android:layout_height="119dp"
                android:layout_below="@+id/f10"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">


                <LinearLayout
                    android:id="@+id/f14"
                    android:layout_width="193dp"
                    android:layout_height="126dp"
                    android:layout_below="@+id/f10"
                    android:layout_marginTop="10dp"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="119dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dp"
                        android:layout_marginTop="10dp"
                        android:text="THERAPY"
                        android:textColor="@android:color/holo_red_dark"
                        android:textSize="18sp"
                        android:textStyle="bold" />


                    <TextView
                        android:layout_width="124dp"
                        android:layout_height="37dp"
                        android:layout_marginLeft="27dp"
                        android:gravity="center"
                        android:text="MANUAL THERAPY"
                        android:textSize="12sp"
                        android:textStyle="bold" />

                    <TextView
                        android:layout_width="110dp"
                        android:layout_height="29dp"
                        android:layout_marginLeft="27dp"
                        android:background="@android:color/white"
                        android:gravity="center"
                        android:text="No Record Found"
                        android:textSize="12sp" />

                </LinearLayout>

                <EditText
                    android:layout_width="133dp"
                    android:layout_height="61dp"
                    android:textAlignment="center"
                    android:layout_marginLeft="23dp"
                    android:layout_marginTop="30dp"
                    android:textSize="12sp"
                    android:textStyle="bold" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/f13"
                android:layout_width="match_parent"
                android:layout_height="144dp"
                android:layout_below="@+id/f11"
                android:layout_marginTop="27dp"
                android:orientation="vertical">


                <TextView
                    android:layout_width="141dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginTop="10dp"
                    android:text="DESCRIPTION"
                    android:textColor="@android:color/holo_red_dark"
                    android:textSize="18sp"
                    android:textStyle="bold" />

                <EditText
                    android:layout_width="353dp"
                    android:layout_height="97dp"
                    android:textAlignment="textStart"
                    android:layout_marginLeft="26dp"
                    android:layout_marginTop="10dp"
                    android:textSize="12sp"
                    android:textStyle="bold" />


            </LinearLayout>


        </RelativeLayout>


    </LinearLayout>
</androidx.cardview.widget.CardView>