<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:backgroundTint="@android:color/white"
    android:paddingBottom="10dp"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <LinearLayout
        android:id="@+id/h1"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:background="#86F1C7"
        android:orientation="horizontal">


        <TextView
            android:layout_width="172dp"
            android:layout_height="53dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/alfaslabone_regular"
            android:text="PHYSIOMATICA"
            android:textColor="#3949AB"
            android:textSize="17dp"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_marginLeft="130dp"
            android:layout_marginTop="15dp"
            android:gravity="center"
            android:src="@drawable/noti"
            android:text="Clinic Name" />




    </LinearLayout>

    <TextView
        android:id="@+id/ui"
        android:layout_width="351dp"
        android:layout_height="100dp"
        android:layout_below="@id/h1"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="35dp"
        android:gravity="center"
        android:textColor="#3949AB"
        android:text="MASTER LISTS"
        android:textSize="20dp"
        android:textStyle="bold" />


    <androidx.cardview.widget.CardView
        android:id="@+id/c1"
        android:layout_width="365dp"
        android:layout_marginTop="180dp"
        android:layout_marginLeft="20dp"
        app:cardBackgroundColor="@android:color/white"
        android:backgroundTint="@android:color/white"
        app:cardCornerRadius="25dp"
        android:layout_height="70dp">
    <LinearLayout
        android:id="@+id/l9"
        android:layout_width="387dp"
        android:backgroundTint="@android:color/white"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <ImageButton
            android:src="@drawable/appointa"
            android:scaleType="centerCrop"
            android:backgroundTint="@android:color/white"
            android:layout_width="50dp"
            android:layout_marginLeft="30dp"
            android:layout_height="50dp"/>


        <TextView
            android:layout_width="128dp"
            android:layout_height="match_parent"
            android:layout_marginStart="28dp"
            android:gravity="center"
            android:text="Treatment"
            android:textColor="@android:color/black"
            android:textSize="15sp"
            android:textStyle="bold" />

        <ImageButton
            android:src="@drawable/rightarrow"
            android:id="@+id/i1"
            android:onClick="ii"
            android:backgroundTint="@android:color/white"
            android:layout_width="50dp"
            android:layout_marginLeft="60dp"
            android:layout_height="50dp"/>

    </LinearLayout>



    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/c2"
        android:layout_below="@+id/c1"
        android:layout_width="365dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        app:cardBackgroundColor="@android:color/white"
        android:backgroundTint="@android:color/white"
        app:cardCornerRadius="25dp"
        android:layout_height="70dp">
        <LinearLayout
            android:id="@+id/l91"
            android:layout_width="387dp"
            android:backgroundTint="@android:color/white"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <ImageButton
                android:src="@drawable/appointa"
                android:scaleType="centerCrop"
                android:backgroundTint="@android:color/white"
                android:layout_width="50dp"
                android:layout_marginLeft="30dp"
                android:layout_height="50dp"/>

            <TextView
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:layout_marginStart="28dp"
                android:gravity="center"
                android:text="Expenses"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/i2"
                android:src="@drawable/rightarrow"
                android:backgroundTint="@android:color/white"
                android:layout_width="50dp"
                android:onClick="n2"
                android:layout_marginLeft="60dp"
                android:layout_height="50dp"/>

        </LinearLayout>



    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/c3"
        android:layout_below="@+id/c2"
        android:layout_width="365dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        app:cardBackgroundColor="@android:color/white"
        android:backgroundTint="@android:color/white"
        app:cardCornerRadius="25dp"
        android:layout_height="70dp">
        <LinearLayout
            android:id="@+id/l92"
            android:layout_width="387dp"
            android:backgroundTint="@android:color/white"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <ImageButton
                android:src="@drawable/appointa"
                android:scaleType="centerCrop"
                android:backgroundTint="@android:color/white"
                android:layout_width="50dp"
                android:layout_marginLeft="30dp"
                android:layout_height="50dp"/>

            <TextView
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:layout_marginStart="28dp"
                android:gravity="center"
                android:text="Income"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/i3"
                android:src="@drawable/rightarrow"
                android:backgroundTint="@android:color/white"
                android:layout_width="50dp"
                android:onClick="n3"
                android:layout_marginLeft="60dp"
                android:layout_height="50dp"/>

        </LinearLayout>



    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/c4"
        android:layout_below="@+id/c3"
        android:layout_width="365dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        app:cardBackgroundColor="@android:color/white"
        android:backgroundTint="@android:color/white"
        app:cardCornerRadius="25dp"
        android:layout_height="70dp">
        <LinearLayout
            android:id="@+id/l95"
            android:layout_width="387dp"
            android:backgroundTint="@android:color/white"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <ImageButton
                android:src="@drawable/appointa"
                android:scaleType="centerCrop"
                android:backgroundTint="@android:color/white"
                android:layout_width="50dp"
                android:layout_marginLeft="30dp"
                android:layout_height="50dp"/>

            <TextView
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:layout_marginStart="28dp"
                android:gravity="center"
                android:text="Modalities"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/i4"
                android:src="@drawable/rightarrow"
                android:backgroundTint="@android:color/white"
                android:layout_width="50dp"
                android:onClick="n4"
                android:layout_marginLeft="60dp"
                android:layout_height="50dp"/>

        </LinearLayout>



    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:id="@+id/c5"
        android:layout_below="@+id/c4"
        android:layout_width="365dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="20dp"
        app:cardBackgroundColor="@android:color/white"
        android:backgroundTint="@android:color/white"
        app:cardCornerRadius="25dp"
        android:layout_height="70dp">
        <LinearLayout
            android:id="@+id/l94"
            android:layout_width="387dp"
            android:backgroundTint="@android:color/white"
            android:layout_height="50dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal">

            <ImageButton
                android:src="@drawable/appointa"
                android:scaleType="centerCrop"
                android:backgroundTint="@android:color/white"
                android:layout_width="50dp"
                android:layout_marginLeft="30dp"
                android:layout_height="50dp"/>

            <TextView
                android:layout_width="130dp"
                android:layout_height="match_parent"
                android:layout_marginStart="28dp"
                android:gravity="center"
                android:text="View All"
                android:textColor="@android:color/black"
                android:textSize="15sp"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/i5"
                android:onClick="n5"
                android:src="@drawable/rightarrow"
                android:backgroundTint="@android:color/white"
                android:layout_width="50dp"
                android:layout_marginLeft="60dp"
                android:layout_height="50dp"/>

        </LinearLayout>



    </androidx.cardview.widget.CardView>













</RelativeLayout>